s" type="text/ng-template"><div class="sites"><div ng-show="!searchText &amp;&amp; !loading &amp;&amp; !viewingRootFolder() &amp;&amp; (!current_folder.sites || !current_folder.sites.length )" data-automation-id="sites-empty-state" class="no-data"><h3>This folder is empty</h3></div><div ng-show="searchText &amp;&amp; filteredSites.length === 0" class="empty"><div class="no-data"><h3>No results for "{{ searchText }}"</h3></div></div><div magic-grid="updateMagicGrid" click-anywhere="click-anywhere" ng-class="{ 'new-site-grid': !legacyUI, 'org-grid': activeOrg(), 'new-project-card-grid': showProjectCardV1 }" class="main-grid site-grid"><div ng-show="!searchText &amp;&amp; viewingRootFolder()" data-automation-id="folder-item" data-name="{{ folder.name }}" ng-repeat="folder in folders" ng-class="{ 'new-site-item': useNewBillingSystem }" class="grid-item site-item folder-item"><div class="inner folder"><div ng-click="setCurrentFolder(folder)" ng-class="{ 'new-project-card': showProjectCardV1 }" class="preview"><div ng-show="!folder.sites || folder.sites.length == 0" class="empty-folder-state"><div class="empty-folder-header">Add sites</div><div class="empty-folder-description">Move sites here from the site menu.</div></div><div ng-repeat="site in folder.thumbs" ng-style="{ backgroundImage: 'url(' + (site.previewUrlSm || site.previewUrl || 'https://d3e54v103j8qbb.cloudfront.net/img/placeholderSm.977272c578.png') + ')' }" class="folder-site-thumb"></div></div></div><div class="footer"><div class="title-wrapper clearfix"><div class="title"><span>{{ folder.name | truncate:25 }}</span></div></div><span class="num-projects">{{ folder.footer }}</span></div></div><div ng-repeat="site in filteredSites" ng-class="{ 'new-site-item': !legacyUI }" data-automation-id="site-item" data-name="{{ site.name }}" class="grid-item site-item"><div ng-class="{ 'non-interactive': !site.active }" class="inner"><div ng-class="{ 'new-preview': !legacyUI, 'new-project-card-preview': showProjectCardV1